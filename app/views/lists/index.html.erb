<div class ="m-4">
  <h1><strong>Save any kind of movies</strong></h1>
</div>
<div class ="m-4">
  <h2><strong>My Lists</strong></h2>
  <ul>
    <% @lists.each do |list| %>
      <li><%= link_to list.name, list_path(list) %></li>
    <% end %>
  </ul>
  <div><%= link_to "Create a new list", new_list_path, class:"btn btn-primary mt-5" %></div>
  </div>
